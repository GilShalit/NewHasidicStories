@using System.Text.Json
    <Div id="mapDiv" style="width: 100%!important;height:100%!important;" />

@code {
    //https://docs.maptiler.com/sdk-js/examples/geojson-markers/
        [Inject] IJSRuntime JSRuntime { get; set; }

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        var json = JsonSerializer.Deserialize<double[]>("[50,50]");
        await JSRuntime.InvokeVoidAsync("showMap", json, "mapDiv");
    }
}
